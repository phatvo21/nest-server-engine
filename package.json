{
  "name": "nest-server-engine",
  "version": "1.0.1",
  "description": "Framework used to develop Backend services with NestJS. This package ships with a lot of features to standardize the creation of services, letting you focus on the business logic.",
  "private": false,
  "engines": {
    "node": ">=16.20.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --color",
    "test": "jest --no-cache --coverage --silent --passWithNoTests --detectOpenHandles --forceExit",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint-staged": "lint-staged",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "start:prod:compose": "docker-compose up --abort-on-container-exit --build",
    "test:compose": "docker-compose -f ./docker-compose.testing.yaml up --remove-orphans --force-recreate --build -d",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "keywords": [
    "nestjs",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phatvo21/nest-server-engine.git"
  },
  "author": "Phat Vo",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/phatvo21/nest-server-engine/issues"
  },
  "dependencies": {
    "@fastify/cookie": "^6.0.0",
    "@fastify/static": "^6.10.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-fastify": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "child_process": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fastify-csrf": "^3.1.0",
    "nestjs-pino": "^3.1.3",
    "pg": "^8.10.0",
    "qs": "^6.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-adjunct": "^4.11.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-auto": "^0.7.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-local": "^1.0.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^34.0.1",
    "eslint-plugin-xss": "^0.1.12",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.2.1",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
